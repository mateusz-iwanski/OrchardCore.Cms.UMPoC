@model UMPoC.Mvc.FacultyAnnouncementModule.ViewModels.FacultyAnnouncementPartViewModel

<div class="form-group">
    <label asp-for="CreatedBy">Created By</label>
    <input asp-for="CreatedBy" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="CreatedUtc">Created At</label>
    <input asp-for="CreatedUtc" class="form-control" type="datetime-local" />
</div>
<div class="form-group">
    <label asp-for="ModifiedBy">Modified By</label>
    <input asp-for="ModifiedBy" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="ModifiedUtc">Modified At</label>
    <input asp-for="ModifiedUtc" class="form-control" type="datetime-local" />
</div>
<div class="form-check">
    <input asp-for="Audit" class="form-check-input" type="checkbox" />
    <label asp-for="Audit" class="form-check-label">Auditing Enabled</label>
</div>
